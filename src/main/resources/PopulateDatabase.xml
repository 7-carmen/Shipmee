		<property name="size" value="XL"/>
		<property name="price" value="3.5"/>
		<property name="route" ref="route1"/>
	</bean>
	
	<!-- RouteOffer -->
	
	<bean id="routeOffer1" class="domain.RouteOffer">
		<property name="amount" value="2.0"/>
		<property name="description" value="Te doy 2€ por un artículo de tamaño L y me estoy arriesgando."/>
		<property name="user" ref="user2"/>
		<property name="route" ref="route1"/>
	</bean>
	
	<!-- Shipment -->
	
	<bean id="shipment1" class="domain.Shipment">
		<property name="date" value="2017/03/09 11:02"/>
		<property name="departureTime" value="2017/03/12 16:00"/>
		<property name="maximumArriveTime" value="2017/03/12 19:00"/>
		<property name="origin" value="Almeria"/>
		<property name="destination" value="Sevilla"/>
		<property name="price" value="2.0"/>
		<property name="itemSize" value="L"/>
		<property name="itemEnvelope" value="Abierto"/>
		<property name="creator" ref="user2"/>
		<!-- <property name="carried" ref=""/> -->
	</bean>
	
	<!-- ShipmentOffer -->
	
	<bean id="shipmentOffer1" class="domain.ShipmentOffer">
		<property name="amount" value="2.5"/>
		<property name="description" value="Te llevo ese artículo por 2.5€ debido a su tamaño."/>
		<property name="user" ref="user1"/>
		<property name="shipment" ref="shipment1"/>
	</bean>
	
	<!-- Notification -->
	
	<bean id="notification1" class="domain.Notification">
		<property name="date" value="2017/03/09 11:02"/>
		<property name="origin" value="Almeria"/>
		<property name="destination" value="Sevilla"/>
		<property name="user" ref="user2"/>
	</bean>
	
	<!-- CreditCard -->
	
	<bean id="creditCard1" class="domain.CreditCard">
		<property name="holderName" value="Guillermo Gamero"/>
		<property name="brandName" value="VISA"/>
		<property name="number" value="4072530828718735"/>
		<property name="expirationMonth" value="07"/>
		<property name="expirationYear" value="2020"/>
		<property name="cvvCode" value="303"/>
